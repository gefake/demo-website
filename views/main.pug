        .carousel-cell {
            display: flex;
            margin: auto;
            align-items: center;
            text-align: center;
            width: 100%;
            height: 380px;
            background-repeat: no-repeat;
            background-position: center;
            background-size: cover;
        }

        .carousel-cell:before {
            display: block;
            text-align: center;
            line-height: 200px;
            font-size: 80px;
            color: white;
        }

        /* no circle */
